$rcsid='$Id: htbrowse.php,v 1.49 2004/01/21 16:19:28 engine Exp engine $ ';
a30 20
function msdbAction()
{
	global $dbMeta;

	if ( $dbMeta->ea == 'msdbInsert' )
		msdbInsert();
	else if ( $dbMeta->ea == 'msdbDelete' )
		msdbDelete();
	else if ( $dbMeta->ea == 'msdbUpdate' )
		msdbUpdate();
	else if ( $dbMeta->ea == 'msdbTest' )
		msdbTest();
	else
		MSDB_ERROR("msdbAction: action :::$dbMeta->ea::: unknown");

	echo "<BR><BR>";
}

