function msdbOthersLink()
{
	global $msdbEnterVar;

	$sid = $msdbEnterVar->sid;
	$sidst = $msdbEnterVar->started;
	$otherLink = "(<A HREF=\"main.php?msdbSID=$sid&msdbSIDST=$sidst\">Other Tables</A>)" ;

	echo "<BR><FONT COLOR='BLUE' size='3'>$otherLink</FONT><BR><BR>\n" ;
}

